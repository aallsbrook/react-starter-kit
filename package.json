{
  "name": "react-starter-kit",
  "version": "1.0.0",
  "description": "A ReactJS/Flux Starter kit that depends on Node, NPM, webpack.",
  "config": {
    "buildDir": "./build",
    "devHost": "localhost",
    "devPort": 8000
  },
  "scripts": {
    "build": "NODE_ENV=production npm run webpack",
    "lint": "eslint --ext .js --ext .jsx ./app ./webpack && echo No linting errors.",
    "start": "NODE_ENV=development node dev-server ./webpack/config",
    "test": "karma start",
    "webpack": "webpack --colors --progress --config ./webpack/config"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lmigpiit/react-starter-kit.git"
  },
  "keywords": [
    "reactjs",
    "flux",
    "starter"
  ],
  "author": "Liberty Mutual",
  "bugs": {
    "url": "https://github.com/lmigpiit/react-starter-kit/issues"
  },
  "homepage": "https://github.com/lmigpiit/react-starter-kit#readme",
  "dependencies": {
    "core-js": "^1.0.1",
    "es6-promise": "^3.0.2",
    "flux": "^2.0.3",
    "immutable": "^3.7.4",
    "lodash": "^3.10.1",
    "normalize.css": "^3.0.3",
    "react": "^0.13.3",
    "react-router": "^0.13.3",
    "react-tap-event-plugin": "^0.1.7",
    "superagent": "^1.3.0"
  },
  "devDependencies": {
    "autoprefixer-core": "^5.2.1",
    "babel": "^5.8.21",
    "babel-core": "^5.8.22",
    "babel-loader": "^5.3.2",
    "babel-runtime": "^5.8.20",
    "css-loader": "^0.16.0",
    "eslint": "^1.1.0",
    "eslint-plugin-react": "^3.2.3",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "json-loader": "^0.5.2",
    "node-sass": "^3.2.0",
    "opn": "^3.0.2",
    "postcss-loader": "^0.5.1",
    "react-hot-loader": "^1.2.8",
    "sass-loader": "^2.0.1",
    "style-loader": "^0.12.3",
    "template-html-loader": "0.0.3",
    "webpack": "^1.11.0",
    "webpack-dev-server": "^1.10.1"
  }
}
